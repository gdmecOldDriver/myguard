<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gdmec.s07150815.myguard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".m1home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".m1home.adapter.HomeAdapter" />
        <activity android:name=".m1home.entity.VersionEntity" />
        <activity android:name=".m1home.utils.DownloadUtils" />
        <activity android:name=".m1home.utils.MyUtils" />
        <activity android:name=".m1home.utils.VersionUpdateUtils" />

        <receiver
            android:name=".m3communicationguard.receiver.InterceptCallReceiver"
            android:enabled="true"
            android:exported="true" />
        <receiver
            android:name=".m3communicationguard.receiver.InterceptSmsReceiver"
            android:enabled="true"
            android:exported="true"></receiver>
    </application>

</manifest>